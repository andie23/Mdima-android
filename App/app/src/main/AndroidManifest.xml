<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mw.forwardplay.mdima">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AreaActivity"
            android:label="Area Menu"
            ></activity>
        <activity android:name=".LocationActivity"
            android:label="Location Menu"
            />
        <activity android:name=".RegionActivity"
            android:label="Region Menu"
            />
        <activity android:name=".ScheduleListActivity"
            android:label="All Schedules"
            />
        <activity android:name=".AreaViewActivity"
            android:label="Area View"
            />
        <activity android:name=".MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>